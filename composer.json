{
  "name": "pfwd/status-model",
  "description": "A status model for Doctrine",
  "type": "library",
  "keywords": ["status","doctrine","orm","entity","entities"],
  "homepage": "https://github.com/pfwd/status-model",
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Fisher",
      "homepage": "http://peterfisher.me.uk",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "PFWD\\Model\\Status\\": "/src/"
    }
  },
  "require": {
    "symfony/console": "~4.0",
    "php": ">=7.2",
    "doctrine/orm": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.11.3",
    "codeception/codeception": "^2.5"
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "symfony-bin-dir": "bin"
  }
}
